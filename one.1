def fajar(time,salah):
    print(f"{salah} Timings i.e {time} Offer Prayers!")
def dhuhar(time,salah):
    print(f"{salah} Timings i.e {time} Offer Prayers!")
def asar(time,salah):
    print(f"{salah} Timings i.e {time} Offer Prayers!")
def maghrib(time,salah):
    print(f"{salah} Timings i.e {time} Offer Prayers!")
def isha(time,salah):
    print(f"{salah} Timings i.e {time} Offer Prayers!")
    
time=int(input("Abi kia time hai bta? (0-24hrs): "))

if time<=4:
    salah="Dhuhar"
    dhuhar(time,salah)
elif time<=6:
    salah="Asar"
    asar(time,salah)
elif time<=7:
    salah="Maghrib"
    maghrib(time,salah)
elif time<=16:
    salah="Isha"
    isha(time,salah)
elif time<=19:
    salah="Fajar"
    fajar(time,salah)
else:
    print("Invailid Time Zone Error")    
    
    
    
    
    